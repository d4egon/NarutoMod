		<description>TODO!</description>
		<spawnThingOnRemoved>Prostheses_ThingDef_NM_Ninshu_Monkey</spawnThingOnRemoved>
		<priceImpact>true</priceImpact>
		<countsAsAddedPartOrImplant>true</countsAsAddedPartOrImplant>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>
		<displayWound>true</displayWound>
		<comps>
			<li Class="NarutoMod.Hediffs.NM_HediffCompProperties_Ability">
				<verbProps>
					
					<li>
						<verbClass>NarutoMod.Verbs.NM_Verb_WoodRelease_DomedWall</verbClass>
						<label>Wood Style: Domed Wall</label>
						<description>TODO!</description>
						<commandIcon>GUI/DomedWall</commandIcon>
						<hasStandardCommand>true</hasStandardCommand>
						<targetable>true</targetable>
						<onlyManualCast>true</onlyManualCast>
						<range>30.0</range>
						<warmupTime>0.5</warmupTime>
						<targetParams>
							<canTargetBuildings>false</canTargetBuildings>
							<canTargetLocations>true</canTargetLocations>
						</targetParams>
						<soundCast>NM_WoodRelease</soundCast>
						<powerCost>30</powerCost>
						<pattern>
							<li>(0, 0)</li>
							<li>(1, 0)</li>
							<li>(-1, 0)</li>
							<li>(0, 1)</li>
							<li>(0, -1)</li>
						</pattern>
					</li>
					
					<li>
						<verbClass>NarutoMod.Verbs.NM_Verb_WoodRelease_DeepForest</verbClass>
						<label>Wood Style: Deep Forest Emerge</label>
						<description>TODO!</description>
						<commandIcon>GUI/DeepForest</commandIcon>
						<hasStandardCommand>true</hasStandardCommand>
						<targetable>true</targetable>
						<onlyManualCast>true</onlyManualCast>
						<range>30.0</range>
						<warmupTime>0.5</warmupTime>
						<targetParams>
							<canTargetBuildings>false</canTargetBuildings>
							<canTargetLocations>true</canTargetLocations>
						</targetParams>
						<soundCast>NM_WoodRelease</soundCast>
						<powerCost>30</powerCost>
						<pattern>
																			<li>(5, -1)</li><li>(5, -2)</li><li>(5, 0)</li><li>(5, 1)</li><li>(5, 2)</li>
															<li>(4, -1)</li><li>(4, -2)</li><li>(4, -3)</li><li>(4, 0)</li><li>(4, 1)</li><li>(4, 2)</li><li>(4, 3)</li>
											<li>(3, -1)</li><li>(3, -2)</li><li>(3, -3)</li><li>(3, -4)</li><li>(3, 0)</li><li>(3, 1)</li><li>(3, 2)</li><li>(3, 3)</li><li>(3, 4)</li>
							<li>(2, -1)</li><li>(2, -2)</li><li>(2, -3)</li><li>(2, -4)</li><li>(2, -5)</li><li>(2, 0)</li><li>(2, 1)</li><li>(2, 2)</li><li>(2, 3)</li><li>(2, 4)</li><li>(2, 5)</li>
							<li>(1, -1)</li><li>(1, -2)</li><li>(1, -3)</li><li>(1, -4)</li><li>(1, -5)</li><li>(1, 0)</li><li>(1, 1)</li><li>(1, 2)</li><li>(1, 3)</li><li>(1, 4)</li><li>(1, 5)</li>
							<li>(0, -5)</li><li>(0, -4)</li><li>(0, -3)</li><li>(0, -2)</li><li>(0, -1)</li><li>(0, 0)</li><li>(0, 1)</li><li>(0, 2)</li><li>(0, 3)</li><li>(0, 4)</li><li>(0, 5)</li>
							<li>(-1, -1)</li><li>(-1, -2)</li><li>(-1, -3)</li><li>(-1, -4)</li><li>(-1, -5)</li><li>(-1, 0)</li><li>(-1, 1)</li><li>(-1, 2)</li><li>(-1, 3)</li><li>(-1, 4)</li><li>(-1, 5)</li>
							<li>(-2, -1)</li><li>(-2, -2)</li><li>(-2, -3)</li><li>(-2, -4)</li><li>(-2, -5)</li><li>(-2, 0)</li><li>(-2, 1)</li><li>(-2, 2)</li><li>(-2, 3)</li><li>(-2, 4)</li><li>(-2, 5)</li>
											<li>(-3, -1)</li><li>(-3, -2)</li><li>(-3, -3)</li><li>(-3, -4)</li><li>(-3, 0)</li><li>(-3, 1)</li><li>(-3, 2)</li><li>(-3, 3)</li><li>(-3, 4)</li>
															<li>(-4, -1)</li><li>(-4, -2)</li><li>(-4, -3)</li><li>(-4, 0)</li><li>(-4, 1)</li><li>(-4, 2)</li><li>(-4, 3)</li>
																			<li>(-5, -1)</li><li>(-5, -2)</li><li>(-5, 0)</li><li>(-5, 1)</li><li>(-5, 2)</li>

						</pattern>
					</li>
					
					<li>
						<verbClass>NarutoMod.Verbs.NM_Verb_SummonThing_WoodClone</verbClass>
						<label>Summoning Jutsu: WoodClone</label>
						<description>TODO!</description>
						<soundCast>NM_SummoningPoof</soundCast>
						<commandIcon>GUI/SummonWoodClone</commandIcon>
						<hasStandardCommand>true</hasStandardCommand>
						<targetable>false</targetable>
						<onlyManualCast>true</onlyManualCast>
						<range>0.0</range>
						<warmupTime>0</warmupTime>
						<targetParams>
							<canTargetBuildings>false</canTargetBuildings>
							<canTargetLocations>false</canTargetLocations>
						</targetParams>
						<powerCost>100</powerCost>
						<pawnKindToSpawn>NM_SummonPawnKind_WoodClone</pawnKindToSpawn>
					</li>
					
				</verbProps>
			</li>
		</comps>
	</HediffDef>

